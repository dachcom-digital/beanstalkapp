{
    "operations": {
        "findAllRepositories": {
            "uri": "repositories.json",
            "httpMethod": "GET",
            "responseModel": "array<Beanstalk\\Model\\RepositoryResponse>"
        },
        "findRepository": {
            "uri": "repositories/{id}.json",
            "httpMethod": "GET",
            "responseModel": "Beanstalk\\Model\\RepositoryResponse",
            "parameters": {
                "id": {
                    "location": "uri",
                    "type": "integer",
                    "required": true
                }
            }
        },
        "createRepository": {
            "uri": "repositories.json",
            "httpMethod": "POST",
            "responseModel": "Beanstalk\\Model\\RepositoryResponse",
            "parameters": {
                "body": {
                    "location": "json",
                    "type": "Beanstalk\\Model\\RepositoryRequest",
                    "required": true
                }
            }

        },
        "updateRepository": {
            "uri": "repositories/{id}.json",
            "httpMethod": "PUT",
            "parameters": {
                "body": {
                    "location": "json",
                    "type": "Beanstalk\\Model\\RepositoryRequest",
                    "required": true
                },
                "id": {
                    "location": "uri",
                    "type": "integer",
                    "required": true
                }
            }
        },
        "findTags": {
            "uri": "repositories/{id}/tags.json",
            "httpMethod": "GET",
            "responseModel": "array<Beanstalk\\Model\\TagResponse>",
            "parameters": {
                "id": {
                    "location": "uri",
                    "type": "integer",
                    "required": true
                }
            }
        },
        "findBranches": {
            "uri": "repositories/{id}/branches.json",
            "httpMethod": "GET",
            "responseModel": "array<Beanstalk\\Model\\BranchResponse>",
            "parameters": {
                "id": {
                    "location": "uri",
                    "type": "integer",
                    "required": true
                }
            }
        }
    }
}
